			<view class="common_cell_bd">
	                <view class="common_cell_bd_p">姓名</view>
			</view>
			<view class="badge" style="top:12px">
	            <input style="text-align: right;" placeholder="请输入您的姓名" class="ship_citychoose" bindinput="keyName" value='{{name}}' maxlength="10">
	            </input>
			</view>
			<view class="with_arrow"></view>
